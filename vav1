    local HardwareId, hwid = {"Syn-Fingerprint", "SW-Fingerprint", "Krnl-Fingerprint"};
    for _,v in next, HardwareId do if decoded["headers"][v] then hwid = decoded["headers"][v];break;end;end;

    setclipboard(hwid)
    
end)

