    LineForce["InverseSquareLaw"] = false 
    LineForce["ApplyAtCenterOfMass"] = true
    LineForce["Magnitude"] = 1e4
    LineForce["MaxForce"] = 1e2 + math.floor(Mass)
    LineForce["ReactionForceEnabled"] = false
    LineForce["Enabled"] = true
